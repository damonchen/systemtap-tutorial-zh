#!/usr/bin/env stap

global count_jiffies, count_ms
probe timer.jiffies(100) {
    count_jiffies++
}

probe timer.ms(100) {
    count_ms++
}

probe timer.ms(12345) {
    hz = (1000*count_jiffies) / count_ms
    printf("jiffies:ms ratio %d:%d => CONFIG_HZ=%d\n", count_jiffies, count_ms, hz)
    exit()
}
